        "express": "^4.18.0",
        "helmet": "^7.0.0",
        "joi": "^17.9.0",
        "jsonwebtoken": "^9.0.0",
        "morgan": "^1.10.0",
        "node-cron": "^3.0.0",
        "nodemailer": "^6.9.0",
        "passport": "^0.6.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.1",
        "prisma": "^5.2.0",
