    "main": "dist/server.js",
    "scripts": {
        "start": "node dist/server.js",
        "dev": "nodemon src/server.ts",
        "dev:https": "set HTTPS=true && nodemon src/server.ts",
        "build": "tsc",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint src --ext .ts",
